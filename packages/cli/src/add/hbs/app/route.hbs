export default {
	path: '{{path}}{{ extra }}',
	name: '{{pathName}}',
	title: '{{title}}',
	{{#if isMobile}}
	components: [
		() => import('./{{childName}}/index.vue')
	]
	{{else if components}}
	components: [
		() => import('./{{childName}}/index.vue'),
		{{#each components}}
		'{{this}}',
		{{/each}}
	]
	{{else if navLevel}}
	components: [
		() => import('./{{childName}}/index.vue'),
		'left',
		'top'
	]
	{{else}}
	components: [
		() => import('./{{childName}}/index.vue'),
		'left'
	]
	{{/if}}
}